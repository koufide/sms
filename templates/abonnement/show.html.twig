{% extends 'base.html.twig' %}

{% block title %}Abonnement{% endblock %}

{% block body %}
    <h1>Abonnement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abonnement.id }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ abonnement.phone }}</td>
            </tr>
            <tr>
                <th>IsActif</th>
                <td>{{ abonnement.isActif ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Motif</th>
                <td>{{ abonnement.motif }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('abonnement_index') }}">back to list</a>

    <a href="{{ path('abonnement_edit', {'id': abonnement.id}) }}">edit</a>

    {{ include('abonnement/_delete_form.html.twig') }}
{% endblock %}
